{
  "name": "unplugin-swc",
  "version": "0.0.0",
  "description": "SWC plugin for Vite and Rollup",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "npm run build && uvu -r esbuild-register --ignore fixtures",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "unplugin": "^0.6.0"
  },
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@rollup/pluginutils": "4.2.1",
    "@swc/core": "1.2.204",
    "@types/node": "17.0.45",
    "defu": "6.0.0",
    "esbuild": "0.14.9",
    "esbuild-register": "3.3.3",
    "load-tsconfig": "0.2.3",
    "path-exists": "5.0.0",
    "prettier": "2.5.1",
    "rollup": "2.75.7",
    "tsup": "5.12.5",
    "typescript": "4.5.4",
    "uvu": "0.5.4",
    "vite": "2.9.12"
  },
  "peerDependencies": {
    "@swc/core": "^1.2.108"
  }
}
